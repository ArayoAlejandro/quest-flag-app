{
    "functions": {
      "api/dist/index.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "builds": [
      {
        "src": "package.json",
        "use": "vercel-sapper"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "frontend/dist/index.html"
      },
      {
        "src": "/.*",
        "dest": "api/dist/index.js"
      }
    ]
  }